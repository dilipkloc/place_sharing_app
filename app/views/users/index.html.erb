<h1> Users List </h1>

<table class="table table-striped ">

  <thead class="thead-dark">
    <tr>
      <th>Email ID</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% if user.email != current_user.email %>
        <tr>
          <td> <%= user.email %> </td>
        </tr>
      <% end %>
    <% end %>
  </tbody>

</table>  